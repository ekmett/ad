distribution:           bionic
no-tests-no-benchmarks: False
unconstrained:          False
hlint:                  True
hlint-job:              9.0.2
-- irc-channels:           irc.freenode.org#haskell-lens
irc-if-in-origin-repo:  True
docspec:                True

constraint-set ffi
  constraints: ad +ffi
  docspec:    True
  benchmarks: True
  tests:      True
  run-tests:  True
  haddock:    True
